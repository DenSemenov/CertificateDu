<Window x:Name="Login" x:Class="CertificateDu.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="797" Width="800" Loaded="Login_Loaded">
    <Grid>
        <Grid HorizontalAlignment="Center" Height="342" Margin="0" VerticalAlignment="Center" Width="366">
            <Label Content="Логин" HorizontalAlignment="Left" Margin="62,98,0,0" VerticalAlignment="Top" FontSize="24"/>
            <ComboBox x:Name="users" HorizontalAlignment="Left" Margin="144,102,0,0" VerticalAlignment="Top" Width="189" FontSize="24"/>
            <Label Content="Пароль" HorizontalAlignment="Left" Margin="38,148,0,0" VerticalAlignment="Top" FontSize="24"/>
            <PasswordBox x:Name="pass" HorizontalAlignment="Left" Margin="144,150,0,0" VerticalAlignment="Top" Width="189" FontSize="24"/>
            <Button x:Name="Log" Content="Войти" HorizontalAlignment="Left" Margin="38,223,0,0" VerticalAlignment="Top" Width="75" FontSize="24" Click="Log_Click"/>
            <Button x:Name="Reg" Content="Регистрация" HorizontalAlignment="Left" Margin="181,223,0,0" VerticalAlignment="Top" Width="152" FontSize="24"/>
        </Grid>
        <Button x:Name="excell" Content="Button" HorizontalAlignment="Left" Margin="146,621,0,0" VerticalAlignment="Top" Width="75" Click="excell_Click" Visibility="Hidden"/>

    </Grid>
</Window>
